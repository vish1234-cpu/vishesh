@model IEnumerable<MvcAssignmentAdvance.Models.Entity.Student>
@{
    ViewBag.Title = "GetStudents";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}

<!Doctype html>
<html>
<head>
    <title></title>
    @Styles.Render("~/bundles/bootstrap")
</head>
<body>
    <div class="container-fluid">
        <div class="container pt-3 p-3 my-3 bg-light text-dark">
            <h1>LIST OF EMPLOYEES WHO GOT REGISTERED THEMSELVES:</h1>
            &nbsp;
            <table class="table table-bordered" style="table-layout:auto">
                <tr>
                    <th>Id</th>
                    <th>First Name </th>
                    <th>Last Name</th>
                    <th>Address </th>
                </tr>
                <tbody>

                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@item.Id</td>
                            <td>@item.FirstName</td>
                            <td>@item.LastName</td>
                            <td>@item.Address</td>
                        </tr>
                    }

                </tbody>
            </table>
        </div>
    </div>
</body>
</html>

